{
  "name": "notes",
  "version": "1.0.0",
  "description": "Notes",
  "main": "server/index.js",
  "scripts": {
    "server": "forever start server/index.js && forever logs -f 0",
    "server:watch": "forever start -w server/index.js && forever logs -f 0",
    "js": "browserify index.js -o dist/index.js",
    "js:watch": "watchify index.js -o dist/index.js -dv",
    "clean": "rm -rf dist && mkdir dist",
    "copy": "cp node_modules/github-markdown-css/github-markdown.css dist/",
    "build": "npm run clean && npm run copy",
    "start": "npm run build && npm run js && npm run server",
    "dev": "npm run build && npm run js:watch & npm run server:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tnguyen14/notes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tnguyen14/notes/issues"
  },
  "homepage": "https://github.com/tnguyen14/notes#readme",
  "dependencies": {
    "bluebird": "^3.4.1",
    "express": "^4.14.0",
    "forever": "^0.15.2",
    "github-markdown-css": "^2.3.0",
    "markdown-it": "^7.0.0",
    "simple-fetch": "^1.2.1",
    "user-home": "^2.0.0"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "watchify": "^3.7.0"
  }
}
